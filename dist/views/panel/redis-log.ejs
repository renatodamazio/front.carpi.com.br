<% var datas = JSON.parse(data); %><section id="redis-log" class="pb-5" v-cloak><div class="container pt-4"><div class="row"><div class="col-lg-6 d-inline"><h1><b>Logs</b></h1></div></div><hr class="mt-3"><div class="row"><div class="col-lg-12"><% if (datas.length >0) { %><ul class="list-group"><% var dataSortedByAsc = datas.sort((a, b) =>-1 * a.created_at.localeCompare(b.created_at)); %><% for (var x = 0; x < dataSortedByAsc.length; x++) { %><li class="list-group-item d-flex justify-content-flex-start align-items-center"><div class="col-lg-2 text-center"><form action="/panel/logs/delete" method="POST"><input type="hidden" value="<%= dataSortedByAsc[x].file %>" name="name"><button type="button" v-if="!del[<%= x %>]" class="btn btn-danger"
                  @click="toggleDel(<%= x %>)">Remover</button><div v-else class="d-inline align-middle"><button type="submit" name="button" class="btn btn-dark">Sim</button>&nbsp;&nbsp;<button class="btn btn-info" @click="toggleDel(<%= x %>)">Não</button></div></form></div><a href="/logs/<%= dataSortedByAsc[x].file %>" download class="col-lg-8 d-inline align-middle"><i class="fa fa-download"></i>&nbsp;&nbsp;&nbsp;&nbsp;<%= dataSortedByAsc[x].file %></a></li><% } %></ul><% } else { %><ul class="list-group text-center"><li class="list-group-item list-group-item-light"><p class="mb-0">Não existem logs!</p></li></ul><% } %></div></div></div></section>